    "@grpc/grpc-js": "^1.8.11",
    "@newrelic/pino-enricher": "^1.1.1",
    "@sinclair/typebox": "^0.25.24",
    "@socket.io/cluster-adapter": "^0.2.1",
    "@socket.io/sticky": "^1.0.3",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "fastify": "^4.13.0",
    "fastify-socket.io": "^4.0.0",
    "google-protobuf": "^3.21.2",
